# NextReach Environment Variables
# Copy this file to .env and fill in your actual values

# DeepSeek API Configuration
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Server Configuration
PORT=3000
NODE_ENV=development

# Security Configuration (Optional - defaults are secure)
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100
CHAT_RATE_LIMIT_WINDOW=60000
CHAT_RATE_LIMIT_MAX=10

# Production Domain (Update for production deployment)
PRODUCTION_DOMAIN=https://yourdomain.com

# Instructions:
# 1. Copy this file to .env
# 2. Replace 'your_deepseek_api_key_here' with your actual DeepSeek API key
# 3. Update PORT if needed (default: 3000)
# 4. Set NODE_ENV=production for production deployment
# 5. Update PRODUCTION_DOMAIN with your actual domain for CORS